{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Web API",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "api_build",
            "program": "${workspaceFolder}/Api/bin/Debug/netcoreapp3.1/Api.dll",
            "args": [],
            "cwd": "${workspaceFolder}/Api",
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart",
            "launchBrowser": {
                "enabled": false,
                "args": "${auto-detect-url}",
                "windows": {
                    "command": "cmd.exe",
                    "args": "/C start ${auto-detect-url}"
                },
                "osx": {
                    "command": "open"
                },
                "linux": {
                    "command": "xdg-open"
                }
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            }
        },
        {
            "name": "Excel Data Insert Console App",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/ExcelDataInsert/bin/Debug/netcoreapp3.1/ExcelDataInsert.dll",
            "args": [],
            "cwd": "${workspaceFolder}/ExcelDataInsert",
            "stopAtEntry": false,
            "console": "internalConsole"
        },
        {
            "name": "ng serve",
            "type": "pwa-chrome",
            "request": "launch",
            "preLaunchTask": "npm: start",
            "url": "http://localhost:4200/"
        },
        {
            "name": "ng test",
            "type": "chrome",
            "request": "launch",
            "preLaunchTask": "npm: test",
            "url": "http://localhost:9876/debug.html"
        }
    ],
    "compounds": [
        {
            "name": "Api/Web",
            "configurations": [
                "Web API",
                "ng serve"
            ]
        }
    ]
}